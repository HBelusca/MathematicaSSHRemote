        RowBox[{"sshCmdMultiplex", ",", "mathCmd"}], "}"}]}], ",", 
      RowBox[{"Asynchronous", "\[Rule]", "True"}]}], " ", 
     RowBox[{"(*", 
      RowBox[{",", 
       RowBox[{"OperatingSystem", "\[Rule]", "\"\<Unix\>\""}]}], "*)"}], 
     "]"}], ",", "4", " ", 
    RowBox[{"(*", " ", 
     RowBox[{
     "In", " ", "multiplex", " ", "mode", " ", "we", " ", "cannot", " ", 
